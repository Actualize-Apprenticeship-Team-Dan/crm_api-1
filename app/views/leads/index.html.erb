<table>
<tr>
  <th>Created</th>
  <th>First Name</th>
  <th>Last Name</th>
  <th>Email</th>
  <th>Phone</th>
  <th>City</th>
  <th>State</th>
  <th>Zip</th>
  <th>Contacted</th>
  <th>Appointment Date</th>
</tr>
<% @leads.each do |lead| %>
  <tr>
    <td><%= lead.created_at %></td>
    <td><%= lead.first_name %></td>
    <td><%= lead.last_name %></td>
    <td><%= lead.email %></td>
    <td><%= lead.phone %></td>
    <td><%= lead.city %></td>
    <td><%= lead.state %></td>
    <td><%= lead.zip %></td>
    <td><%= lead.contacted %></td>
    <td><%= lead.appointment_date %></td>
  </tr>
<% end %>
</table>
