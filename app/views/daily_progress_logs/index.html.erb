<section class="forms">
  <div class="container-fluid">
    <header>
      <h1>Daily Progress Logs</h1>
    </header>


    <div class="row">
      <table class="table">
        <th>
          <td>Date</td>
          <td>Processed</td>
          <td>Connects</td>
          <td>Sets</td>
        </th>
        <% @logs.each do |log| %>
          <tr>
            <td><%= log.date %></td>
            <td><%= log.processed %></td>
            <td><%= log.connects %></td>
            <td><%= log.sets %></td>
          </tr>
        <% end %>
      </table>
    </div>


  </div>
</section>